<div class='info-title'>
    <h1>Car Make</h1>
</div>
<div class='edit-info-container'>
    <h4>Select Make</h4>
    <div class='edit-car-info-links'>
        <ul>
        <% @car_makes.each do |car_make| %>
            <%= link_to edit_car_make_path(car_make) do %>
                <li><%= car_make.name %></li>
            <% end %>
        <% end %>
        </ul>
    </div>
</div>